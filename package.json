{
  "name": "root",
  "version": "1.0.0",
  "scripts": {
    "install": "npm install --prefix backend && npm install --prefix frontend",
    "build": "npm run build --prefix backend && npm run build --prefix frontend",
    "migrate": "cd backend && npx sequelize-cli db:migrate",
    "seed:all": "cd backend && npx sequelize-cli db:seed:all",
    "seed:undo:all": "cd backend && npx sequelize-cli db:seed:undo:all",
    "migrate:undo:all": "cd backend && npx sequelize-cli db:migrate:undo:all"
  },
  "dependencies": {
    "nodemailer": "^6.9.15"
  }
}
